%semantic token 'c' 
%{
use Data::Dumper;
$Data::Dumper::Indent = 1;
%}
%%
S:  'c'+  D+ 
      { 
         print Dumper($_[1]);
         print Dumper($_[2]);
      } 
;

D: 'd'
     {
       bless { attr => $_[1], children =>[]}, 'DES';
     }
;

%%

use base 'Tail';
__PACKAGE__->lexer(\&Tail::lex);

sub Run {
  my ($self) = shift;
  return $self->YYParse( yybuildingtree => 1 );
}
